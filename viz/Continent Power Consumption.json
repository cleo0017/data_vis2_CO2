{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Global Power Consumption",
  "width": 500,
  "height": 500,
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/cleo0017/data_vis2_CO2/main/data/Continent_Power_Consumption.csv"
      },
      "mark": "area",
      "encoding": {
        "x": {
          "field": "Year",
          "type": "temporal",
          "title": "Time",
          "axis": {"grid": false}
        },
        "y": {
          "field": "Power Consumption",
          "type": "quantitative",
          "title": "Power Consumption",
          "axis": {"grid": false}
        },
        "color": {"field": "Continent", "type": "nominal", "legend": null},
        "row": {"field": "Continent", "type": "nominal", "title": "Global Organisation"}
      }
    }
  ],
  "config": {}
}
